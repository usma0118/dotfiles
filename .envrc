#shellcheck disable=SC2148,SC2155
export ANSIBLE_CONFIG=$(expand_path ./ansible.cfg)
export ANSIBLE_HOST_KEY_CHECKING="False"
# Ann Seabelle
export ANSIBLE_LOCALHOST_WARNING="False"
export ANSIBLE_COLLECTIONS_PATH=$(expand_path ~/.venv/galaxy)
export ANSIBLE_VARS_ENABLED="host_group_vars"
export ANSIBLE_INVENTORY_UNPARSED_WARNING="False"
# github
export GITHUB_USER=${GITHUB_USER:-usma0118}
# Venv
PATH_add "$(expand_path ~/.venv/bin)"
export VIRTUAL_ENV="$(expand_path ~/.venv)"
export PYTHONDONTWRITEBYTECODE="1"

export DOTFILES_DIR="$(expand_path .)"
